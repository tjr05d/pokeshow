<div class="container">
  <h2>The Pokeshow is LIT</h2>

  <div class="slideShow">
    <% @pokes.each do |poke | %>
      <div class="card">
        <h3><%= poke.name %> </h3>

        <div class="image">
          <img src= <%=poke.img_url %> alt="poke image"/>
        </div>

        <h4><%= poke.lyrics %></h4>

        <button id= "like"> Like</button>
      </div>
    <% end %>
  </div>
</div>
